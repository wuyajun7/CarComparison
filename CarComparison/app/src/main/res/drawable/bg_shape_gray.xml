<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#FFFFFF" />
            <corners />
            <stroke
                android:width="0.1px"
                android:color="#dadada" />
        </shape>
    </item>
    <item
        android:bottom="1px"
        android:right="1px">
        <shape>
            <solid android:color="#FFFFFF" />
            <corners
                android:topLeftRadius="0.1px"
                android:topRightRadius="0.1px" />
            <stroke
                android:width="0.1px"
                android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>
